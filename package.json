{
  "name": "todo-nestjs-app",
  "version": "0.0.1",
  "description": "Production-ready NestJS boilerplate with TypeScript",
  "author": "",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "quality": "pnpm lint:check && pnpm format:check && pnpm test && pnpm build",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install; fi",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "docker:build": "docker build -t nestjs-boilerplate .",
    "docker:build:prod": "docker build --target runner -t nestjs-boilerplate:prod .",
    "docker:run": "docker run -p 3000:3000 --env-file .env nestjs-boilerplate",
    "docker:run:prod": "docker run -p 3000:3000 --env-file .env nestjs-boilerplate:prod",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:compose:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:compose:down": "docker-compose -f docker-compose.dev.yml -f docker-compose.prod.yml down"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "@supabase/supabase-js": "^2.0.0",
    "@google-cloud/storage": "^7.0.0",
    "minio": "^8.0.0",
    "multer": "^2.0.2",
    "sharp": "^0.32.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cls-rtracer": "^2.0.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.4",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.11.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.4",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
